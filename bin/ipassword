#!/usr/bin/env python
import random

def make_password(groups):
	COUNT = 5
	password = ""
	group = random.choice(groups)
	for g in group:
		c = ''.join(random.sample(g, COUNT))
		password += c
		print(c)
	print
	return password

if __name__ == "__main__":
	punctuation = '-/:;()$&@".,?!'
	alpha = 'abcdefghijklmnopqrstuvwxyz'
	ALPHA = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'
	numbers = '0123456789'

	groups = [[alpha, ALPHA, punctuation, numbers], [alpha, ALPHA, numbers, punctuation]]

	print(make_password(groups))
