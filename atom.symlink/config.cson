      "Sublime-Style-Column-Selection"
      "duplicate-line-or-selection"
      "emmet"
      "emmet-jsx-css-modules"
      "language-babel"
      "language-jade"
      "teletype"
    ]
    hideGitIgnoredFiles: true
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "*node_modules*"
      "ui.content"
      "target"
      "META-INF"
      "api.apps"
      "config"
      "core"
      ""
    ]
    projectHome: "~/bin"
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "limited"
    themes: [
      "city-lights-ui"
      "city-lights-syntax"
    ]
  editor:
    fontFamily: "Fira Code"
    fontSize: 15
