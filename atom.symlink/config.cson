      "text.html.basic text.html.twig"
    ]
  "markdown-preview":
    useGitHubStyle: true
  "merge-conflicts": {}
  minimap:
    absoluteMode: true
    adjustAbsoluteModeHeight: true
  react: {}
  "release-notes":
    viewedVersion: "0.89.0"
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
      "text.html.basic"
    ]
  "svg-preview":
