"*":
  "atom-beautify":
    general:
      _analyticsUserId: "e426b16e-4437-4291-a506-5fa5465e114d"
    scss:
      beautify_on_save: true
      convert_quotes: "single"
      force_indentation: true
      indent_size: 2
      no_lead_zero: true
      preserve_newlines: true
  "atom-clock":
    showClockIcon: true
  autosave:
    enabled: true
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "minimap"
      "linter"
      "linter-docker"
      "autocompiler-pug-jade"
      "linter-pug"
