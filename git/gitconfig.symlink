[core]
    quotepath = false
    excludesfile = /Users/Steven/.gitignore
[user]
    name = Steven Bone
    email = stevenmbone@gmail.com
[color]
    ui = true
[alias]
    mylog = log --author=Steven
    l = "!source ~/.githelpers && pretty_git_log"
    st = status
    co = checkout
    sp = stash pop
    di = diff
    dc = diff --cached
    dt = difftool
[difftool "Kaleidoscope"]
    cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[difftool]
    prompt = false
[diff]
    tool = Kaleidoscope
